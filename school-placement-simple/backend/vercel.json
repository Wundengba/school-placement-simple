{
  "version": 2,
  "buildCommand": "npm install && npx prisma generate && node scripts/migrateDirect.js",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**", "prisma/**", "node_modules/.prisma/**"],
        "serverlessFunctionTimeout": 60
      }
    }
  ],
  "env": {},
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}
